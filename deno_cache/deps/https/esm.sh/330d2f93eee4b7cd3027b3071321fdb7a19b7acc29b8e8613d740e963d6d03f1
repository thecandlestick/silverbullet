/* esm.sh - esbuild bundle(lib0@0.2.74/time) deno production */
var n=Math.floor;var c=Math.round,a=Math.log10;var l=Number.isNaN;var p=t=>Math.pow(10,t);var i=["","k","M","G","T","P","E","Z","Y"],f=["","m","\u03BC","n","p","f","a","z","y"],h=(t,e=0)=>{let r=t===0?0:a(t),o=0;for(;r<o*3&&e>-8;)e--,o--;for(;r>=3+o*3&&e<8;)e++,o++;let s=e<0?f[-e]:i[e];return{n:c((o>0?t/p(o*3):t*p(o*-3))*1e12)/1e12,prefix:s}};var u=()=>new Date,w=Date.now,M=t=>{if(t<6e4){let x=h(t,-1);return c(x.n*100)/100+x.prefix+"s"}t=n(t/1e3);let e=t%60,r=n(t/60)%60,o=n(t/3600)%24,s=n(t/86400);return s>0?s+"d"+(o>0||r>30?" "+(r>30?o+1:o)+"h":""):o>0?o+"h"+(r>0||e>30?" "+(e>30?r+1:r)+"min":""):r+"min"+(e>0?" "+e+"s":"")};export{u as getDate,w as getUnixTime,M as humanizeDuration};
